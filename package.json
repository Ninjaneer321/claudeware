{
  "name": "claude-code-wrapper",
  "version": "0.1.0",
  "description": "Query collection wrapper for Claude Code CLI with token optimization",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "claude-code-wrapper": "./src/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest test/integration --testPathPattern=integration",
    "test:manual": "node test/manual/quick-test.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/cli.ts",
    "benchmark": "tsx scripts/benchmark.ts",
    "link": "npm link",
    "unlink": "npm unlink"
  },
  "keywords": [
    "claude",
    "cli",
    "wrapper",
    "optimization",
    "token"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "mock-spawn": "^0.2.6",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.45.1",
    "better-sqlite3": "^9.2.2",
    "lru-cache": "^10.1.0",
    "opossum": "^8.1.3",
    "pino": "^8.17.1",
    "stream-json": "^1.8.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "scripts/postinstall.ts"
  ]
}
